  pipeline {
    agent {
      label 'master'
    }  
    
    stages {
     stage('build') {
         steps {
            // build
                 sh 'ant -f build.xml -v'
          
         }    
     }
     stage('Unit Tests') {
       steps {
         sh 'ant -f test.xml -v'
         junit 'reports/result.xml'
       }
     }
    }
  
