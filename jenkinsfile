  pipeline {
    agent any

    stages {
      stage('Unit Tests') {
         steps {
            // Unit Tests
                 sh 'ant -f test.xml -v'
                    junit 'reports/results.xml'
          
         }    
      }
     
      stage('build') {
         steps {
            // build
              sh 'ant -f build.xml -v'
         }
       }
    }
   
    post {
      always {
        archiveArtifacts artifacts: 'dist/*.jar', fingerprint: true
      } 
    }
  }
